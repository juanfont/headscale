// This ACL is used to test group expansion

{
    "groups": {
        "group:example": [
            "testnamespace",
        ],
    },

    "hosts": {
        "host-1": "100.100.100.100",
        "subnet-1": "100.100.101.100/24",
    },

    "acls": [
        {
            "action": "accept",
            "src": [
                "group:example",
            ],
            "dst": [
                "host-1:*",
            ],
        },
    ],
}